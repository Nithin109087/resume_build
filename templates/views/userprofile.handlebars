<!-- ##### Contact Area Start ##### -->
<section class="section-padding-100 contact_us_area" id="contact">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="section-heading text-center">
                    <!-- Dream Dots -->
                    <div class="dream-dots justify-content-center wow fadeInUp" data-wow-delay="0.2s">
                        <span></span><span></span><span></span><span></span><span></span><span></span><span></span>
                    </div>
                    <h2 class="wow fadeInUp" data-wow-delay="0.3s">User Profile</h2>
                </div>
                <a href="/users/update">Edit Profile </a>
            </div>
        </div>

        <!-- Contact Form -->
        <div class="row justify-content-center">
            <div class="col-12 col-md-10 col-lg-8">
                <div class="contact_form">

                    <div class="row">
                        <div class="col-sm-3">
                            <h3 class="mb-0"> Name</h3>
                        </div>
                        <div class="col-sm-12 font700">{{nameOfUser}}</div>
                    </div>

                    <div class="row">
                        <div class="col-sm-3">
                            <h3 class="mb-0"> Email</h3>
                        </div>
                        <div class="col-sm-12 font700">{{email}}</div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3">
                            <h3 class="mb-0"> phoneNumber</h3>
                        </div>
                        <div class="col-sm-12 font700">{{phoneNumber}}</div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3">
                            <h3 class="mb-0"> userName</h3>
                        </div>
                        <div class="col-sm-12 font700">{{userName}}</div>
                    </div>
                    <div class="row">
                        <div class="col-sm-3">
                            <h3 class="mb-0"> LinkedIn</h3>
                        </div>
                        <div class="col-sm-12 font700">{{linkedin}}</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- ##### Contact Area End ##### -->